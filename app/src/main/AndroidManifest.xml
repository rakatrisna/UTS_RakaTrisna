<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raka.myrukunsalatapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyRukunSalatApp">
        <activity
            android:name="com.raka.myrukunsalatapp.DetailActivity"
            android:exported="true"
            android:parentActivityName="com.raka.myrukunsalatapp.MainActivity"/>
        <activity
            android:name="com.raka.myrukunsalatapp.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.raka.myrukunsalatapp.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>